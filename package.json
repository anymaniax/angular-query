{
  "name": "azahi",
  "version": "0.0.2",
  "description": "Performant and powerful remote data synchronization for Angular",
  "author": "anymaniax",
  "license": "MIT",
  "repository": "azahi",
  "homepage": "https://github.com/anymaniax/azahi#readme",
  "scripts": {
    "ng": "ng",
    "build": "ng build --prod && npm run copy",
    "dev": "npm run build -- --watch",
    "test": "ng test",
    "lint": "ng lint",
    "format": "prettier --write '**/*.{js,ts,scss,json,md}'",
    "copy": "npm run copy:readme && npm run copy:license && npm run copy:logo",
    "copy:readme": "cpr README.md dist/azahi/README.md --overwrite",
    "copy:license": "cpr LICENSE dist/azahi/LICENSE --overwrite",
    "copy:logo": "cpr azahi-og.png dist/azahi/azahi-og.png --overwrite"
  },
  "release": {
    "branches": [
      "main",
      "next"
    ]
  },
  "dependencies": {
    "@angular/animations": "~11.0.5",
    "@angular/common": "~11.0.5",
    "@angular/compiler": "~11.0.5",
    "@angular/core": "~11.0.5",
    "@angular/forms": "~11.0.5",
    "@angular/platform-browser": "~11.0.5",
    "@angular/platform-browser-dynamic": "~11.0.5",
    "@angular/router": "~11.0.5",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.5",
    "@angular-eslint/builder": "1.0.0",
    "@angular-eslint/eslint-plugin": "1.0.0",
    "@angular-eslint/eslint-plugin-template": "1.0.0",
    "@angular-eslint/schematics": "1.0.0",
    "@angular-eslint/template-parser": "1.0.0",
    "@angular/cli": "~11.0.5",
    "@angular/compiler-cli": "~11.0.5",
    "@angular/language-service": "~11.0.5",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "cpr": "^3.0.1",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "husky": "^4.3.6",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lint-staged": "^10.5.3",
    "ng-packagr": "^11.0.3",
    "prettier": "^2.2.1",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "typescript": "~4.0.5"
  },
  "keywords": [
    "rest",
    "client",
    "fetch",
    "data fetching",
    "angular",
    "azahi",
    "react-query"
  ]
}
